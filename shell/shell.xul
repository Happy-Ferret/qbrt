<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>
<?xml-stylesheet href="main.css" type="text/css"?>

<window id="main-window"
        windowtype="navigator:browser"
        xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <script type="application/javascript" src="shell.js"/>

<panel id="optionspane"
       noautohide="true"
       persist="screenX screenY"
       width="1024"
       height="768"
       noautofocus="false"
       level="parent"
       consumeoutsideclicks="true"
       titlebar="none"
       type="content"
       backdrag="true">
       <box>
        <label flex="0" id="titleText" class="frameButton" value="titlebar"/>
</box>
       <browser id="content" type="content-primary" flex="2" />
</panel>


</window>
